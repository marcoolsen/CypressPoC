{
    "name": "cypresspoc",
    "version": "1.0.0",
    "description": "Poc using cypress and ts",
    "main": "index.js",
    "scripts": {
        "cypress:api": "cypress run --spec 'cypress/e2e/API/**/*'",
        "cypress:fe": "cypress run --spec 'cypress/e2e/FE/**/*'",
        "cypress:all": "cypress run",
        "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\""
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/marcoolsen/CypressPoC.git"
    },
    "author": "Marco Olsen",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/marcoolsen/CypressPoC/issues"
    },
    "homepage": "https://github.com/marcoolsen/CypressPoC#readme",
    "devDependencies": {
        "cypress": "^12.14.0",
        "cypress-mochawesome-reporter": "^3.5.0",
        "prettier": "2.8.8",
        "typescript": "^5.1.3"
    }
}
